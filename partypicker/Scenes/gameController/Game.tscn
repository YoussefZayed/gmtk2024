script/source = "extends Node2D

@export var battleControllerScene: PackedScene
@export var mainMenuScene: PackedScene
@export var settings_menu: PackedScene
@export var ui_overlay: PackedScene
@export var legend_ui: PackedScene
@export var expansionSelectScene: PackedScene 

@export var shopScreenScene: PackedScene 
@export var postBattleScreenScene: PackedScene
@export var rewardButtonScene: PackedScene
@export var relicsListScene: PackedScene

@export var playerChars: Array[Entity]
@export var BD_Chars: Array[Entity]
@export var BS_Chars: Array[Entity]
@export var HB_Chars: Array[Entity]
@export var COTW_Chars: Array[Entity]
@export var ROS_Chars: Array[Entity]
@export var DD_Chars: Array[Entity]
@export var KC_Chars: Array[Entity]
@export var Wretch: Array[Entity]

@export var commonRelicPosibilities: Array[Relic]
@export var rareRelicPosibilities: Array[Relic]
@export var epicRelicPosibilities: Array[Relic]
@export var legendaryRelicPosibilities: Array[Relic]
@export var cursedRelicPosibilities: Array[Relic]

@export var EnemyChars: Array[Entity]

var battleController: BattleController
var mainMenu: Node
var settingsMenu: Node
var legendUI: Node
var UIoverlay: Node
var expansionSelect: Node

var shopScreen: Node
var rewardsScreen: Node
var goldButton: Node
var relicButtonCommon: Node
var relicButtonRare: Node
var relicButtonEpic: Node
var relicButtonLegendary: Node
var relicButtonCursed: Node
var relicCommon: Relic
var relicRare: Relic
var relicEpic: Relic
var relicLegendary: Relic
var relicCursed: Relic
var relicsList: Node

var timeTrial = false

var players: Array[Entity] = []
var ids = 1;
var level = 1;
var choices: Array[Entity] = []
@export var boss: Entity
var inBattle = false

var goldStored = 0
var goldPosibilities = [10, 20, 30, 40, 50]
var aquiredRelics: Array[Relic]

# Called when the node enters the scene tree for the first time.
func _ready() -> void:
	mainMenu = mainMenuScene.instantiate()
	print(mainMenu)
	self.add_child(mainMenu)
	settingsMenu = settings_menu.instantiate()
	self.add_child(settingsMenu)
	legendUI = legend_ui.instantiate()
	self.add_child(legendUI)
	UIoverlay = ui_overlay.instantiate()
	self.add_child(UIoverlay)
	relicsList = relicsListScene.instantiate()
	self.add_child(relicsList)
	
	mainMenu.find_child(\"New Game\").connect('pressed',startGame)
	mainMenu.find_child(\"Icon Legend Button\").connect('pressed',showLegendMenu)
	mainMenu.find_child(\"Options Button\").connect('pressed',showOptionsMenu)
	UIoverlay.find_child(\"Settings Button\").connect('pressed',showOptionsMenu)
	UIoverlay.find_child(\"Party View Button\").connect('pressed',showPartyList)
	UIoverlay.find_child(\"Relic Bag View Button\").connect('pressed',showRelicList)
	
	settingsMenu.find_child(\"Settings Canvas\").visible = false
	legendUI.find_child(\"Legend Canvas\").visible = false
	UIoverlay.find_child(\"UI Elements Canvas\").visible = false
	relicsList.find_child(\"Relic List Canvas\").visible = false

func showTavernBar():
	if (self.get_child_count() > 7):
		print(self.get_child_count())
		return
	shopScreen = shopScreenScene.instantiate()
	choices = []
	choices.append(playerChars.pick_random())
	choices.append(playerChars.pick_random())
	choices.append(playerChars.pick_random())
	# Character Select Buttons
	var PC1 = shopScreen.find_child(\"PARTY_CHOICE_1\")
	var PC2 = shopScreen.find_child(\"PARTY_CHOICE_2\")
	var PC3 = shopScreen.find_child(\"PARTY_CHOICE_3\")
	var IdentifyRelics = shopScreen.find_child(\"Identify Relics\")
	
	PC1.text= choices[0].name
	PC1.tooltip_text = choices[0].tool_tip
	PC1.connect(\"pressed\",addChoice0)
	PC2.text= choices[1].name
	PC2.tooltip_text = choices[1].tool_tip
	PC2.connect(\"pressed\",addChoice1)
	PC3.text= choices[2].name
	PC3.tooltip_text = choices[2].tool_tip
	PC3.connect(\"pressed\",addChoice2)
	
	IdentifyRelics.connect(\"pressed\",indentifyAllRelics)
	
	# Location Select Buttons
	shopScreen.find_child(\"Continue Quest\").disabled = true
	shopScreen.find_child(\"Continue Quest\").connect(\"pressed\",continueQuest)
	print([\"Aquired Relics ->\", aquiredRelics])
	if len(players) > 0:
		shopScreen.find_child(\"Continue Quest\").disabled = false
	if len(players) > 0:
		shopScreen.find_child(\"Continue Quest\").text = \"Continue Adventure\"
	if len(aquiredRelics) > 0:
		shopScreen.find_child(\"Identify Relics\").disabled = false
	
	self.add_child(shopScreen)

func showBattleRewards():
	rewardsScreen = postBattleScreenScene.instantiate()
	self.add_child(rewardsScreen)
	rewardsScreen.find_child(\"End Rewards\").connect(\"pressed\",resolveRewards)
	addGoldReward()
	var rng = RandomNumberGenerator.new() # WIP stuff for random generation.
	var randomValue = rng.randf_range(0,5)
	if (randomValue>0) and (randomValue<3.5):
		addRelicRewardCommon()
	if (randomValue>1) and (randomValue<4):
		addRelicRewardRare()
	if randomValue>2:
		addRelicRewardEpic()
	if randomValue>3:
		addRelicRewardLegendary()
	if randomValue>4:
		addRelicRewardCursed()


func addGoldReward():
	goldButton = rewardButtonScene.instantiate()
	var goldAmount = goldPosibilities.pick_random()
	rewardsScreen.find_child(\"Button Vertical Container\").add_child(goldButton)
	goldButton.connect(\"pressed\",collectGold)
	goldButton.find_child(\"Reward Label\").text = str(goldAmount)

func collectGold():
	var goldAdd = int(UIoverlay.find_child(\"Gold\").text) + int(goldButton.find_child(\"Reward Label\").text)
	goldStored = goldAdd
	UIoverlay.find_child(\"Gold\").text = str(goldAdd)
	if rewardsScreen.find_child(\"Button Vertical Container\").get_child_count() == 1:
		rewardsScreen.find_child(\"End Rewards\").text = \"Continue\"
	goldButton.queue_free()

func addRelicRewardCommon():
	relicButtonCommon = rewardButtonScene.instantiate()
	relicCommon = commonRelicPosibilities.pick_random()
	rewardsScreen.find_child(\"Button Vertical Container\").add_child(relicButtonCommon)
	relicButtonCommon.connect(\"pressed\",collectRelicCommon)
	relicButtonCommon.find_child(\"Reward Label\").text = str(relicCommon.public_name)
	relicButtonCommon.find_child(\"Reward Texture\").texture = relicCommon.public_texture

func collectRelicCommon():
	aquiredRelics.append(relicCommon)
	addRelicToBag(relicCommon)
	if rewardsScreen.find_child(\"Button Vertical Container\").get_child_count() == 1:
		rewardsScreen.find_child(\"End Rewards\").text = \"Continue\"
	relicButtonCommon.queue_free()

func addRelicRewardRare():
	relicButtonRare = rewardButtonScene.instantiate()
	relicRare = commonRelicPosibilities.pick_random()
	rewardsScreen.find_child(\"Button Vertical Container\").add_child(relicButtonRare)
	relicButtonRare.connect(\"pressed\",collectRelicRare)
	relicButtonRare.find_child(\"Reward Label\").text = str(relicRare.public_name)
	relicButtonRare.find_child(\"Reward Texture\").texture = relicRare.public_texture

func collectRelicRare():
	aquiredRelics.append(relicRare)
	addRelicToBag(relicRare)
	if rewardsScreen.find_child(\"Button Vertical Container\").get_child_count() == 1:
		rewardsScreen.find_child(\"End Rewards\").text = \"Continue\"
	relicButtonRare.queue_free()

func addRelicRewardEpic():
	relicButtonEpic = rewardButtonScene.instantiate()
	relicEpic = commonRelicPosibilities.pick_random()
	rewardsScreen.find_child(\"Button Vertical Container\").add_child(relicButtonEpic)
	relicButtonEpic.connect(\"pressed\",collectRelicEpic)
	relicButtonEpic.find_child(\"Reward Label\").text = str(relicEpic.public_name)
	relicButtonEpic.find_child(\"Reward Texture\").texture = relicEpic.public_texture

func collectRelicEpic():
	aquiredRelics.append(relicEpic)
	addRelicToBag(relicEpic)
	if rewardsScreen.find_child(\"Button Vertical Container\").get_child_count() == 1:
		rewardsScreen.find_child(\"End Rewards\").text = \"Continue\"
	relicButtonEpic.queue_free()

func addRelicRewardLegendary():
	relicButtonLegendary = rewardButtonScene.instantiate()
	relicLegendary = commonRelicPosibilities.pick_random()
	rewardsScreen.find_child(\"Button Vertical Container\").add_child(relicButtonLegendary)
	relicButtonLegendary.connect(\"pressed\",collectRelicLegendary)
	relicButtonLegendary.find_child(\"Reward Label\").text = str(relicLegendary.public_name)
	relicButtonLegendary.find_child(\"Reward Texture\").texture = relicLegendary.public_texture

func collectRelicLegendary():
	aquiredRelics.append(relicLegendary)
	addRelicToBag(relicLegendary)
	if rewardsScreen.find_child(\"Button Vertical Container\").get_child_count() == 1:
		rewardsScreen.find_child(\"End Rewards\").text = \"Continue\"
	relicButtonLegendary.queue_free()

func addRelicRewardCursed():
	relicButtonCursed = rewardButtonScene.instantiate()
	relicCursed = cursedRelicPosibilities.pick_random()
	rewardsScreen.find_child(\"Button Vertical Container\").add_child(relicButtonCursed)
	relicButtonCursed.connect(\"pressed\",collectRelicCursed)
	relicButtonCursed.find_child(\"Reward Label\").text = str(relicCursed.public_name)
	relicButtonCursed.find_child(\"Reward Texture\").texture = relicCursed.public_texture

func collectRelicCursed():
	aquiredRelics.append(relicCursed)
	addRelicToBag(relicCursed)
	if rewardsScreen.find_child(\"Button Vertical Container\").get_child_count() == 1:
		rewardsScreen.find_child(\"End Rewards\").text = \"Continue\"
	relicButtonCursed.queue_free()

func addRelicToBag(relic: Relic):
	var relicButton = rewardButtonScene.instantiate()
	relicsList.find_child(\"Button Vertical Container\").add_child(relicButton)
	relicButton.find_child(\"Reward Label\").text = str(relic.public_name)
	relicButton.find_child(\"Reward Texture\").texture = relic.public_texture

func indentifyAllRelics():
	for button in relicsList.find_child(\"Button Vertical Container\").get_children():
		button.queue_free()
	for relicItem in aquiredRelics:
		relicItem.identify()
		addRelicToBag(relicItem)

func resolveRewards():
	rewardsScreen.queue_free()
	showTavernBar()

func showExpansionSelect():
	expansionSelect = expansionSelectScene.instantiate()
	self.add_child(expansionSelect)
	expansionSelect.find_child(\"Start Game Button\").connect(\"pressed\",transferExpansionData)

func transferExpansionData():
	if expansionSelect.find_child(\"Basic Damage Button\").button_pressed:
		playerChars.append_array(BD_Chars)
	if expansionSelect.find_child(\"Basic Support Button\").button_pressed:
		playerChars.append_array(BS_Chars)
	if expansionSelect.find_child(\"Honorbound Button\").button_pressed:
		playerChars.append_array(HB_Chars)
	if expansionSelect.find_child(\"Call of the Wild Button\").button_pressed:
		playerChars.append_array(COTW_Chars)
	if expansionSelect.find_child(\"Rise of Slayers Button\").button_pressed:
		playerChars.append_array(ROS_Chars)
	if expansionSelect.find_child(\"Dark Dealings Button\").button_pressed:
		playerChars.append_array(DD_Chars)
	if expansionSelect.find_child(\"Kings Court Button\").button_pressed:
		playerChars.append_array(KC_Chars)
	if len(playerChars) == 0:
		playerChars.append_array(Wretch)
	if expansionSelect.find_child(\"Time Trial Button\").button_pressed:
		timeTrial = true
	expansionSelect.queue_free()
	showTavernBar()

func addChoice0():
	var player = Entity.new()
	player.load_from_resource(choices[0])
	player.id = str(ids)
	ids+=1
	players.append(player)
	if len(players) > 0:
		shopScreen.find_child(\"Continue Quest\").disabled = false
		disableAddCharacters()

func addChoice1():
	var player = Entity.new()
	player.load_from_resource(choices[1])
	player.id = str(ids)
	ids+=1
	players.append(player)
	if len(players) > 0:
		shopScreen.find_child(\"Continue Quest\").disabled = false
		disableAddCharacters()

func addChoice2():
	var player = Entity.new()
	player.load_from_resource(choices[2])
	player.id = str(ids)
	ids+=1
	players.append(player)
	if len(players) > 0:
		shopScreen.find_child(\"Continue Quest\").disabled = false
		disableAddCharacters()

func disableAddCharacters():
	shopScreen.find_child(\"PARTY_CHOICE_1\").disabled = true
	shopScreen.find_child(\"PARTY_CHOICE_2\").disabled = true
	shopScreen.find_child(\"PARTY_CHOICE_3\").disabled = true

func continueQuest():
	startBattle()

func startBattle():
	inBattle = true
	var items = getBattleControllerItems()
	battleController = battleControllerScene.instantiate()
	battleController.init(items[0],items[1],items[2], timeTrial)
	battleController.connect(\"player_died\",player_died)
	battleController.connect(\"battle_ended\",battleEnded)
	self.add_child(battleController)
	level += 1;
	if (is_instance_valid(shopScreen)):
		shopScreen.queue_free()
	
func player_died(id):
	if not inBattle:
		pass
	inBattle = false
	for i in players: #range(len(players)):
		if i.id == id:
			players.erase(i)
	if len(players) == 0:
		gameOver()
		
func battleEnded():
	if not inBattle:
		pass
	inBattle = false
	print(\"👍\")
	battleController.queue_free()
	if level == 6:
		print(\"Condition 1\")
		$GameOverWin.visible = true
		$\"GameOverWin/Win Container/HBoxContainer/Restart\".connect(\"pressed\",restart)
		$\"GameOverWin/Win Container/HBoxContainer/Exit Game\".connect(\"pressed\",quit)
	else:
		if len(players) < 4 && len(players)>0:
			showBattleRewards()
		elif len(players)>0:
			startBattle()
		

func gameOver():
	battleController.queue_free()
	$GameOverLose.visible = true
	$\"GameOverLose/Lose Container/HBoxContainer/Restart\".connect(\"pressed\",restart)
	$\"GameOverLose/Lose Container/HBoxContainer/Exit Game\".connect(\"pressed\",quit)

func quit():
	get_tree().quit()
	
func restart():
	self.get_tree().reload_current_scene()
	
	

func startGame():
	print(\"STARTED!\")
	UIoverlay.find_child(\"UI Elements Canvas\").visible = true
	showExpansionSelect()
	mainMenu.queue_free()

func showOptionsMenu():
	settingsMenu.find_child(\"Settings Canvas\").visible = true

func showLegendMenu():
	legendUI.find_child(\"Legend Canvas\").visible = true

func showPartyList():
	pass

func showRelicList():
	relicsList.find_child(\"Relic List Canvas\").visible = !relicsList.find_child(\"Relic List Canvas\").visible
	

func _input(event: InputEvent) -> void: # disabled until ordering fix
	if event.is_action_pressed(\"ui_cancel\"):
		settingsMenu.find_child(\"Settings Canvas\").visible = !settingsMenu.find_child(\"Settings Canvas\").visible
	if event.is_action_pressed(\"l_pressed\"):
		legendUI.find_child(\"Legend Canvas\").visible = !legendUI.find_child(\"Legend Canvas\").visible


func addPlayer(resource: Resource):
	var player = Entity.new()
	player.load_from_resource(resource)
	player.id = str(ids)
	ids+=1
	players.append(player)

func getEnemies() -> Array[Entity]:
	var enemies: Array[Entity] = []
	for i in range(max(1, min(len(players), level))):
		var enemy = Entity.new()
		enemy.load_from_resource(EnemyChars.pick_random())
		enemy.id = str(ids)
		ids+=1
		enemies.append(enemy)
	return enemies

func getBattleControllerItems():
	var enemies : Array[Entity] =getEnemies()
	if level == 5:
		enemies = [boss]
	enemies.shuffle()
	var battles: Array[Battle] = []
	for player in players:
		var battle = Battle.new(player, enemies.pick_random())
		battles.append(battle)
	return [players+enemies, battles, 5+ 4*len(players)] 
		
	
